
<StimulationSet>

  <!--onset and duration in ms, rate in particles/ms-->
<!--fast injection to have sharp increase-->
<InjectionStim specieID="Ca" injectionSite="pointA">
  
        <onset>               201000          </onset>
        <duration>        10         </duration> 
        <rate>             180000       </rate>
	<period>        1100             </period>
	<end>          202000           </end>
	<interTrainInterval> 300000     </interTrainInterval>
	<numTrains>           4       </numTrains>
</InjectionStim>

<!--maintained desire concentration-->
<InjectionStim specieID="Ca" injectionSite="pointA">
  
        <onset>               201020          </onset>
        <duration>        10         </duration> 
        <rate>            20010       </rate>
	<period>        320         </period>
	<end>        202000        </end>
	<interTrainInterval> 300000     </interTrainInterval>
	<numTrains>           4       </numTrains>
</InjectionStim>

<!--buff to bring back Ca to basal. repeated -->
<InjectionStim specieID="Cbuff" injectionSite="pointA">

        <onset>               202000          </onset>
        <duration>        5         </duration> 
      	<rate>       420000 </rate>
	<period>        1000             </period>
	<end>          203000           </end>
	<interTrainInterval> 300000     </interTrainInterval>
	<numTrains>           4       </numTrains>
</InjectionStim>



